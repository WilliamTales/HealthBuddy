  "flows": [
    {
      "name": "Protection",
      "uuid": "c939b94e-a76e-40fd-ba1b-68d04903c754",
      "spec_version": "13.1.0",
      "language": "eng",
      "type": "messaging",
      "_ui": {
        "nodes": {
          "2d63a014-e6cb-44c6-9e6b-94554131487f": {
            "position": {
              "left": 0,
              "top": 0
            },
            "type": "execute_actions"
          }
